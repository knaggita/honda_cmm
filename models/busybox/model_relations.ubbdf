<?xml version="1.0"?>
<relations>
    <relation name="turn_on_light" type="light">
        <parent_joint name="prismatic_slider"/>
        <child_joint name="fixed_light"/>
        <parameters threshold="0.1" off_color="0.3 0.3 0 1" on_color="1 1 0 1"/>
    </relation>

    <relation name="change_spinner_handle" type="light">
        <parent_joint name="prismatic_slider"/>
        <child_joint name="fixed_spinner_handle"/>
        <parameters threshold="-0.1" off_color="0 1 0 1" on_color="1 0 0 1"/>
    </relation>

    <!--relation name="enable_spinner" type="enable">
        <parent_joint name="prismatic_slider"/>
