        <parameters lower_limit="-0.2" upper_limit="0.0"/>
    </relation-->

    <relation name="move_door_handle" type="door">
        <parent_joint name="revolute_door_handle"/>
        <child_joint name="prismatic_door_latch"/>
        <parameters/>
    </relation>
</relations>
